<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GuardarFragment">

<android.support.constraint.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imgGuardar1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="150dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5"
        app:srcCompat="@android:drawable/editbox_dropdown_dark_frame" />

    <ImageButton
        android:id="@+id/imgGuardar2"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/imgGuardar1"
        app:layout_constraintTop_toTopOf="@+id/imgGuardar1"
        app:srcCompat="@android:drawable/editbox_dropdown_dark_frame" />

    <ImageButton
        android:id="@+id/imgGuardar3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/imgGuardar5"
        app:layout_constraintTop_toTopOf="@+id/imgGuardar5"
        app:srcCompat="@android:drawable/editbox_dropdown_dark_frame" />

    <ImageButton
        android:id="@+id/imgGuardar4"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="@+id/imgGuardar1"
        app:layout_constraintTop_toBottomOf="@+id/imgGuardar1"
        app:srcCompat="@android:drawable/editbox_dropdown_dark_frame" />

    <ImageButton
        android:id="@+id/imgGuardar5"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/imgGuardar4"
        app:layout_constraintTop_toTopOf="@+id/imgGuardar4"
        app:srcCompat="@android:drawable/editbox_dropdown_dark_frame" />

    <ImageButton
        android:id="@+id/imgGuardar6"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/imgGuardar2"
        app:layout_constraintTop_toTopOf="@+id/imgGuardar2"
        app:srcCompat="@android:drawable/editbox_dropdown_dark_frame" />

    <TextView
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="Name"
        android:textColor="#61b1a9"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.151" />


</android.support.constraint.ConstraintLayout>

</FrameLayout>